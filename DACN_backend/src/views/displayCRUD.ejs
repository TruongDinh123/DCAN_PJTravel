<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Get CRID</title>
  </head>
  <style>
    table,
    th,
    td {
      border: 1px solid black;
    }
  </style>
  <body>
    <h2>HTML Table</h2>

    <table style="width: 100%">
      <tr>
        <th>username</th>
        <th>email</th>
        <th>password</th>
      </tr>
      <% for(let i = 0; i < dataTable.length; i++) { %>
      <tr>
        <td><%= dataTable[i].username%></td>
        <td><%= dataTable[i].password%></td>
        <!-- <td><%= dataTable[i].fullName%></td> -->
        <td><%= dataTable[i].email%></td>
        <!-- <td><%= dataTable[i].phoneNumber%></td> -->
        <!-- <td><%= dataTable[i].image%></td> -->
        <!-- <td><%= dataTable[i].roleID%></td> -->
      </tr>
      <% } %>
    </table>
  </body>
</html>
